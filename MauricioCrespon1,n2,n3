#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;

void main()
{
	int numero1,numero2,numero3;// Declarar variables
		
	cout<<"Ingrese el numero1: "<<endl;
	cin>>numero1;
	cout<<"Ingrese el numero2: "<<endl;
	cin>>numero2;
	cout<<"Ingrese el numero3: "<<endl;
	cin>>numero3;
	if (numero1>=numero2)
		if (numero1>=numero3)
			cout<<" El mayor es: "<<numero1<<endl;
		else
			cout<<" El mayor es: "<<numero3<<endl;
	else
		if (numero2>=numero3)
			cout<<"El mayor es: "<<numero3<<endl;
	getch();
}
